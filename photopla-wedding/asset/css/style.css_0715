/*
*トップ
*/
@keyframes scale {
  50% {
    transform: translate(-10px, 10px) scale(1.02) rotate(3deg);
  }
  100% {
    transform: scale(1) rotate(0);
  }
}
@keyframes scale-reverse {
  50% {
    transform: translate(10px, -10px) scale(0.98) rotate(-3deg);
  }
  100% {
    transform: scale(1);
  }
}
#top {
  width: 100%;
  height: 100vh;
  padding: 76px 0 0; /* ←TODO: 修正 */
  position: relative;
}


.top_kv {
  position: relative;
  height: 100%;
  background-image: url("../image/top/kv.png");
  animation: scale-reverse 6s ease-in-out infinite;
  background-size: 110%; /* ←TODO: 修正 */
}
#top .mask {
  height: 100%;
  max-width: 380px;
  margin: 0 auto;
  -webkit-mask-image: url("../image/top/blob.svg");
  -webkit-mask-repeat: no-repeat;
  -webkit-mask-position: center 10px;
  -webkit-mask-size: 97%; /* ←TODO: 修正 */
  mask-image: url("../image/top/blob.svg");
  mask-repeat: no-repeat;
  mask-position: center 10px;
  mask-size: 97%; /* ←TODO: 修正 */
  animation: scale 6s ease-in-out infinite;
}
#top .studio {
  position: absolute;
  top: 88px;
  left: 50%;
  transform: translateX(-50%);
