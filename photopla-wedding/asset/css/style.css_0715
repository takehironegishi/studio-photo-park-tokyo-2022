}
#top .kv_text_left {
  position: absolute;
  top: 580px; /* ←TODO: 修正 */
  left: 20px;
  font-size: 12px;
}
#top .kv_text_right {
  position: absolute;
  top: 580px; /* ←TODO: 修正 */
  right: 20px;
  font-size: 12px;
}
#top .kv_text_center {
  position: absolute;
  bottom: 0;
  left: 50%;
