/*
*トップ
*/
@keyframes scale {
  50% {
    transform: translate(-10px, 10px) scale(1.02) rotate(3deg);
  }
  100% {
    transform: scale(1) rotate(0);
  }
}
@keyframes scale-reverse {
  50% {
    transform: translate(10px, -10px) scale(0.98) rotate(-3deg);
  }
  100% {
    transform: scale(1);
  }
}
@keyframes initialAnimation {
  0%,
  20% {
    -webkit-mask-size: 160%;
    -webkit-mask-position: center -70px;
    mask-position: center -70px;
    mask-size: 160%;
  }
  100% {
    -webkit-mask-size: 92%;
    -webkit-mask-position: center 10px;
    mask-position: center 10px;
    mask-size: 92%;
  }
}
#top {
  width: 100%;
  height: 100vh;
  padding: 76px 0 0;
  position: relative;
}
.top_kv {
  position: relative;
  height: 100%;
  background-image: url("../image/top/kv.webp");
  background-size: 100%;
}
.scale-reverse {
  animation: scale-reverse 4s ease-in-out infinite;
}
.initial-animation {
  animation: initialAnimation 3s ease-out forwards;
}
#top .mask {
  /* border-right: 18px solid #fff;  大阪の場合のみ←を追加 */
  height: 100%;
  max-width: 380px;
  margin: 0 auto;
  -webkit-mask-image: url("../image/top/blob.svg");
  -webkit-mask-repeat: no-repeat;
  -webkit-mask-position: center 10px;
  -webkit-mask-size: 85%;
  mask-image: url("../image/top/blob.svg");
  mask-repeat: no-repeat;
  mask-position: center 10px;
  mask-size: 85%;
}
.border-right {
  border-right: 18px solid #fff;
}
.scale {
  animation: scale 4s ease-in-out infinite;
}
#top .studio {
  position: absolute;
  top: 88px;
  left: 50%;
  transform: translateX(-50%);
  font-size: 46px;
}
#top .photo_park {
  position: absolute;
  top: 140px;
  left: 50%;
  transform: translateX(-50%);
  font-size: 52px;
  white-space: nowrap;
  font-family: "Bodoni Bold";
}
#top .kv_text_left {
  position: absolute;
  top: 580px;
  left: 20px;
  font-size: 12px;
}
#top .kv_text_right {
  position: absolute;
  top: 580px;
  right: 20px;
  font-size: 12px;
}
#top .kv_text_center {
  position: absolute;
  bottom: -40px;
  left: 50%;
  transform: translateX(-50%);
  text-align: center;
  font-size: 20px;
  width: 100%;
  white-space: nowrap;
}

