/*
*
* ページ共通 
*
*/

/*
* Util
*/
@font-face {
  font-family: "Bodoni";
  src: url("../fonts/bodoni/BodoniFLF-Roman.ttf");
}
@font-face {
  font-family: "Bodoni Bold";
  src: url("../fonts/bodoni/BodoniFLF-Bold.ttf");
}
@font-face {
  font-family: "Yonaga";
  src: url("../fonts/YonagaOldMincho/YonagaOldMincho-Regular.ttf");
}
@font-face {
  font-family: "Julius";
  src: url("../fonts/Julius/JuliusSansOne-Regular.ttf");
}
.contain-no-repeat {
  background-repeat: no-repeat;
  background-size: contain;
}
.flex-between {
  display: flex;
  justify-content: space-between;
}
.bold-title {
  font-size: 60px;
  font-weight: bold;
  letter-spacing: 4px;
}
.title_text {
  font-size: 20px;
  line-height: 1.8;
}
.bodoni {
  font-family: "Bodoni";
}
.bodoni--bold {
  font-family: "Bodoni Bold";
}
.yonaga {
  font-family: "Yonaga";
}
.julius {
  font-family: "Julius";
}
.only-pc {
  display: none;
}
.only-sp {
  display: block;
}
.js-fadeIn {
  opacity: 0;
  visibility: hidden;
  transform: translateY(30px);
  transition: opacity 1.5s, visibility 1.5s, transform 1.5s;
}
.is-fadeIn {
  opacity: 1;
  visibility: visible;
  transform: translateY(0);
}
.steep {
  transform: rotate(-5deg);
}
.active {
  opacity: 1;
}

/*
* header
*/
header {
  position: fixed;
  top: 0;
  left: 0;
  align-items: center;
  width: 100%;
  padding: 16px 16px 8px 24px;
  background-color: rgba(255, 255, 255, 0.7);
  z-index: 10;
}
.header_logo {
  width: 30vw;
}
.openbtn {
  position: relative;
  cursor: pointer;
  width: 50px;
  height: 50px;
  z-index: 3;
}
.openbtn span {
  display: inline-block;
  transition: all 0.4s;
  position: absolute;
  left: 14px;
  height: 2px;
  border-radius: 2px;
  background: #707070;
}
.openbtn span:nth-of-type(1) {
  top: 15px;
  width: 45%;
}
.openbtn span:nth-of-type(2) {
  top: 23px;
  width: 35%;
}
.openbtn span:nth-of-type(3) {
  top: 31px;
  width: 20%;
}
/*activeクラスが付与されると線が回転して×になる*/
.openbtn.active span:nth-of-type(1) {
  top: 18px;
  left: 18px;
  transform: translateY(6px) rotate(-135deg);
  width: 35%;
  height: 1px;
}
.openbtn.active span:nth-of-type(2) {
  opacity: 0;
}
.openbtn.active span:nth-of-type(3) {
  top: 30px;
  left: 18px;
  transform: translateY(-6px) rotate(135deg);
  width: 35%;
  height: 1px;
}
/* ハンバーガーメニューオープン時のスタイル */
nav {
  position: fixed;
  top: 0;
  left: 100%;
  width: 100%;
  height: 100%;
  padding: 64px;
  background-color: #fff;
  transition: all 0.4s;
  overflow: scroll;
}
nav.open {
  left: 0;
}
nav ul {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  row-gap: 28px;
  margin-bottom: 48px;
}
nav ul li a {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.nav_text {
  font-size: 20px;
}
.nav_subtext {
  margin-bottom: 2px;
  font-size: 12px;
  color: #ccc;
  font-family: "Yonaga";
}
.nav_btn {
  width: 100%;
  font-size: 15px;
  padding: 15px 0;
  margin-bottom: 20px;
  color: #000;
}
.primary {
  border: none;
  background: #e6dab9;
}
.secondary {
  border: 1px solid #baa878;
  background-color: #fff;
}

main {
  background: #fff;
}

/*
*
* TOPページ
