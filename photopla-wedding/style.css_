/*
* Galleryページ
* (一部流用できるスタイルは他のページのものを流用している)
*/

/* TOP KV */
#gallery #top {
  width: 100vw;
  height: 100vh;
  margin-bottom: 56px;
  padding: 76px 0 0;
}
#gallery .top_kv {
  max-width: 420px;
  height: 100%;
  margin: 0 auto;
  background-image: url("../image/gallery/tokyo/top/kv.jpeg");
  background-size: cover;
  background-position: -224px top;
  animation: none;
}
#gallery .top_kv.osaka {
  background-image: url("../image/gallery/osaka/top/kv.jpeg");
}

/* TOPKV下の説明エリア */
#gallery #phrase {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 0 auto;
  max-width: 500px;
  padding: 0 40px;
}
#gallery #phrase .description {
  margin-bottom: 40px;
  font-size: 12px;
  line-height: 2;
  text-align: center;
}
#gallery #phrase .catch-phrase {
  margin-bottom: 40px;
  color: #958832;
  font-size: 18px;
  text-align: center;
}
#gallery #phrase .link_wrapper {
  display: flex;
  flex-wrap: wrap;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: calc(100% + 32px);
  padding-left: 0;
}
#gallery #phrase .link_wrapper a {
  display: block;
  width: 50%;
}
#gallery #phrase .link_wrapper p {
  position: relative;
  margin-bottom: 44px;
  font-size: 10px;
}
#gallery #phrase .link_wrapper p::before {
  content: "";
  position: absolute;
  top: 50%;
  left: auto;
  right: 11%;
  transform: translateY(-50%);
  width: 40px;
  height: 1px;
  background-color: #333;
}
#gallery #phrase .link_wrapper p::after {
  content: "";
  position: absolute;
  top: 50%;
  left: auto;
  right: 1vw;
  transform: translateY(-50%);
  width: 26px;
  height: 26px;
  background-color: transparent;
  border: 1px solid #333;
  border-radius: 50%;
}
#gallery #phrase .link_wrapper a.gallery-link p::before,
#gallery #phrase .link_wrapper a.gallery-link p::after {
  content: none;
}
#gallery #phrase .link_wrapper a.gallery-link p {
  position: relative;
  padding: 8px 32px;
  border: 1px solid #707070;
  text-align: center;
}
#gallery #phrase .link_wrapper a.gallery-link p span {
  position: absolute;
  right: 20px;
  top: 50%;
  transform: translateY(-70%) rotate(45deg);
  display: inline-block;
  width: 6px;
  height: 6px;
  border-right: 2px solid #707070;
  border-bottom: 2px solid #707070;
}

/* 各スタジオ */
#gallery .studio {
  max-width: 500px;
  margin: 0 auto 40px;
  padding-bottom: 8px;
  background-repeat: no-repeat;
  background-position: center 160px;
  background-size: cover;
  background-color: rgba(255, 255, 255, 0.7);
  background-blend-mode: lighten;
}
#gallery #dramatic-studio {
  background-image: url(../image/gallery/tokyo/dramatic/11.jpg);
}
#gallery #kachofugetsu-studio {
  background-image: url(../image/gallery/tokyo/kachofugetsu/1.jpg);
}
#gallery #lounge-studio {
  background-image: url(../image/gallery/tokyo/lounge/15.jpg);
}
#gallery #motion-studio {
  background-image: url(../image/gallery/tokyo/motion/19.jpg);
}
#gallery #kachofugetsu-studio--osaka {
  background-image: url(../image/gallery/osaka/kachofugetsu/1.jpg);
}
#gallery #komichi-studio {
  background-image: url(../image/gallery/osaka/komichi/9.jpg);
}
#gallery #gallery-studio {
  background-image: url(../image/gallery/osaka/gallery-scene/11.jpg);
}
#gallery #dress-studio {
  background-image: url(../image/gallery/osaka/dress-scene/13.jpg);
}
#gallery #motion-studio--osaka {
  background-image: url(../image/gallery/osaka/motion/17.jpg);
}
#gallery .studio .studio_shoot {
  margin-bottom: 32px;
}
#gallery .studio_shoot_title {
  position: relative;
  margin-left: 16px;
  font-size: 18px;
}
#gallery .studio_shoot_title span {
  margin-left: 8px;
  display: block;
  margin-top: 4px;
  font-size: 10px;
}
#gallery .studio_shoot_title_border {
  position: absolute;
  left: -24px;
  bottom: -6px;
  width: 240px;
  border-bottom: 1px solid #ccc;
}
#gallery .studio .swiper-container {
  margin: 0 !important;
  box-shadow: none;
  overflow: hidden;
}
#gallery .studio .swipe-content {
  padding: 12px;
  border: 1px solid #c0c0c0;
  background-color: #fff;
}
#gallery .studio .swipe-content-title {
  display: flex;
  align-items: flex-end;
  margin-bottom: 8px;
}
#gallery .studio .swipe-content-title p:first-child {
  position: relative;
  margin-right: 8px;
  font-size: 10px;
}
#gallery .studio .swipe-content-title p:first-child::before {
  content: "";
  position: absolute;
  left: 0;
  bottom: 2px;
  width: 16px;
  height: 0.5px;
  background-color: #c0c0c0;
}
#gallery .studio .swipe-content-title p:first-child span {
  margin-left: 2px;
