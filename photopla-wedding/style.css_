}
#gallery .studio .swiper-button-wrapper {
  display: flex;
  justify-content: flex-end;
  column-gap: 12px;
  margin-top: 12px;
  padding-right: 12px;
}
#gallery .studio .swiper-button-wrapper .swiper-button-prev,
#gallery .studio .swiper-button-wrapper .swiper-button-next {
  position: static;
  width: 24px;
  height: 24px;
  margin-top: 0;
}
#gallery .studio .swiper-button-wrapper .swiper-button-prev::after,
#gallery .studio .swiper-button-wrapper .swiper-button-next::after {
  content: none;
}

/* フォトギャラリー */
#gallery #photo-gallery {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin: 80px auto 0;
  padding: 0 12px;
  max-width: 500px;
}
#gallery #photo-gallery h2 {
  position: relative;
  font-size: 35px;
}
#gallery #photo-gallery h2::after {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: -8px;
  content: "";
  width: 48px;
  height: 2px;
  background-color: #ccc;
}
#gallery #photo-gallery h3 {
  margin-top: 35px;
  font-size: 13px;
}
#gallery #photo-gallery .first-gallery-wrapper {
  margin-top: 40px;
}
#gallery #photo-gallery .last-gallery-wrapper {
  display: none;
  margin-top: 8px;
}
#gallery #photo-gallery .grid-photos-wrapper {
  display: grid;
  grid-template-columns: repeat(3, 112px);
  grid-template-rows: repeat(3, 112px);
  gap: 8px;
}
#gallery #photo-gallery .grid-photos-wrapper .grid-photoitem-wrapper {
  width: 112px;
  height: 112px;
}
#gallery #photo-gallery .grid-photos-wrapper .grid-photoitem-wrapper img {
  width: 100%;
  height: 100%;
  object-fit: cover;
}
#gallery #photo-gallery .button-wrapper {
  display: flex;
  justify-content: center;
  margin-top: 32px;
}
#gallery #photo-gallery .button-wrapper button {
  width: 170px;
  padding: 8px 0;
  background: #fff;
  border: 1px solid #222;
  font-size: 12px;
  cursor: pointer;
}
#gallery .lb-outerContainer {
  background-color: inherit;
}
#gallery .lightbox .lb-image {
  width: 80vw !important;
  max-width: 400px;
  height: auto !important;
}

